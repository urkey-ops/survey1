{
  "functions": {
    "api/submit-survey.js": {
      "runtime": "@vercel/node@20.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/submit-survey",
      "destination": "/api/submit-survey.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
